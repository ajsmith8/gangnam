<div class = "vote container">
	<div class = "vote up"><div class = "small-arrow"></div></div>
	<div class = "vote count">2</div>
	<div class = "vote down"><div class = "small-darrow"></div></div>
</div>
<div class = "title container">
	<div class = "title">
		<b><%= fact.get('title') %></b>
	</div>
</div>
<div class = "description container">
	<div class = "description"></div>
</div>
<div class = "sources container" id = "sources"></div>
<div class = "extras"></div>
<div class = "stats">
	<div class = "picture">
		<img src = "assets/fusegap.png" height = "100%" />
	</div>
	<div class = "text">
		<div class = "name">John</div>
		<div class = "rank">Rank: 40 of 200</div>
	</div>
	<div class = "share tooltip"><span class = "share-tip">Share</span><img src = "assets/facebook64.png" width = "100%" /></div>
	<div class = "edit tooltip" id = "fedit"><span class = "edit-tip">Edit</span><img src = "assets/cog.png" width = "100%" /></div>
	<div class = "comments tooltip">
		<span class = "comment-tip">Comment</span>
		<img src = "assets/bubble.png" width = "100%" />
		<div class = "comments count">3</div>
	</div>
</div>