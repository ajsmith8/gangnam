<div id = "question" style = "margin: 0 0 10px 0"></div>
<% if (facts.length > 0) { %>
	<div class = "fact-header">
		<% if (facts.length !== 1) { %>
			<%= facts.length %> Facts
		<% } else { %>
			<%= facts.length %> Fact
		<% } %>
	</div>
<% } %>
<div id = "facts"></div>
<div class = "fact-add">Add a Fact</div>
<div id = "create"></div>