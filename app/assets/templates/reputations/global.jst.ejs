<div class = "profile-picture">
	<div class = "profile-picture-wrapper">
		<% if (user.get('is_temp_user')) { %>
			<img src = "assets/logos/blue.png" height = "100%" />
		<% } else { %>
			<% if (user.get('provider') === 'facebook') { %>
				<img src = "https://graph.facebook.com/<%= user.get('uid') %>/picture" height = "100%" />
			<% } else if (user.get('provider') === 'google') { %>
				<img src = "assets/logos/blue.png" height = "100%" />
			<% } else { %>
				<img src = "http://api.twitter.com/1/users/profile_image/<%= user.get('uid') %>?size=bigger" height = "100%" />
			<% } %>
		<% } %>
	</div>
</div>
<div class = "profile-text-container">
	<div class = "profile-text">
		Profile: 
		<span class = "profile-name">
			<% if (user.get('is_temp_user')) { %>
				Anon
			<% } else { %>
				<%= user.get('name') %>
			<% } %>
		</span>
	</div>
	<div class = "profile-text">Rank: 
		<% if (rank.rank !== null) { %>
			<span class = "profile-name"><%= rank.rank %> of <%= rank.users %></span>
		<% } else { %>
			Unranked
		<% } %>
	</div>
</div>

