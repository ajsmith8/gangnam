<div id = "issue_show" class = "thumbnail">
	<div class = "thumbnail-wrapper">
		<% if (issue.get('thumbnail')) { %>
			<img src = "assets/issues/<%= issue.get('thumbnail') %>" height = "100%"/>
		<% } else { %>
			<img src = "assets/logos/black.png" height = "100%" />
		<% } %>
	</div>
</div>
<div id = "issue_show" class = "title container">
	<div class = "title text"><%= issue.get('title') %></div>
</div>
<div id = "user_info"></div>
<div id = "achievements"></div>

