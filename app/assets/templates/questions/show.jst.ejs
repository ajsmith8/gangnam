<div class = "inner">	
	<div class = "vote container">
		<div class = "vote up"><div class = "arrow"></div></div>
		<div class = "vote count">4</div>
		<div class = "vote down"><div class = "darrow"></div></div>
	</div>
	<div class = "triangle-border bubble">
		<div class = "title container">
			<div class = "title">
				<b><%= question.get('title') %></b>
			</div>
		</div>
	</div>
	<div class = "stats">
		<div class = "picture">
			<img src = "https://graph.facebook.com/100003671661574/picture" height = "100%" />
		</div>
		<div class = "text">
			<div class = "name">Joe</div>
			<div class = "rank">Rank: 142 of 173</div>
		</div>
		<div class = "share tooltip"><span class = "share-tip">Share</span><img src = "assets/facebook64.png" width = "100%"/></div>
		<div class = "edit tooltip" id = "quedit"><span class = "edit-tip">Edit</span><img src = "assets/gear.png" width = "100%" /></div>
		<div class = "comments tooltip">
			<span class = "comment-tip">Comment</span>
			<img src = "assets/comment.png" width = "100%" />
			<div class = "comments count"></div>
		</div>
	</div>
</div>
<div id = "extras"></div>
