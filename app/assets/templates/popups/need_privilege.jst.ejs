<div class = "popup wrapper">
	<div id = "popup" class = "pops-container">
		<div class = "pops-text">
			<%= privilege.get('kind').split(' ')[1] %> requires <%= privilege.get('rep') %> reputation 
			<span class = "learn-more" id = "more">(learn more)</span> 
		</div>
		<div class = "pops-thumbnail">
			<div class = "big-x">&#x2717;</div>
			<img src = "assets/achievements/<%= privilege.get('thumbnail') %>" height = "100%" />
		</div>
		<div class = "pops-clickme">
			(click to dismiss)
		</div>
	</div>
</div>