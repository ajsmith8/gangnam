<div class = "popup wrapper">
	<div id = "popup" class = "pops-container">
		<div class = "pops-text">
			You've unlocked the ability to <%= privilege.get('kind').split(' ')[0] %>!
		</div>
		<div class = "pops-thumbnail">
			<div class = "amount"></div>
			<img src = "assets/achievements/<%= privilege.get('thumbnail') %>" height = "100%" />
		</div>
		<div class = "pops-clickme">
			(click to dismiss)
		</div>
	</div>
</div>